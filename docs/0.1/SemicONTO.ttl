@prefix : <http://w3id.org/semiconto/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dcterms:contributor a owl:AnnotationProperty .

dcterms:created a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:isVersionOf a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:modified a owl:AnnotationProperty .

dcterms:rights a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

<http://qudt.org/schema/qudt/QuantityKind> a owl:Class .

: a owl:Ontology ;
    dcterms:created "2023-03-21" ;
    dcterms:creator "Huanyu Li" ;
    dcterms:license "https://github.com/huanyu-li/SemicONTO/blob/main/LICENSE" ;
    dcterms:title "Semiconductor ONTOlogy (SemicONTO)" ;
    vann:preferredNamespacePrefix "semiconto" ;
    vann:preferredNamespaceUri "http://w3id.org/SemicONTO/0.1/" ;
    rdfs:seeAlso "https://github.com/huanyu-li/SemicONTO" ;
    owl:versionIRI <http://w3id.org/semiconto/0.1/> ;
    owl:versionInfo "0.1" .

:AbsorptionCofficient a owl:Class ;
    rdfs:subClassOf :OpticalProperty .

:AtmosphericPressureCondition a owl:Class ;
    rdfs:subClassOf <http://qudt.org/schema/qudt/Quantity>,
        :Condition .

:BreakdownField a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:BulkModulus a owl:Class ;
    rdfs:subClassOf :ThermalProperty .

:ChemicalSubstance a owl:Class ;
    rdfs:seeAlso "http://emmo.info/emmo#EMMO_df96cbb6_b5ee_4222_8eab_b3675df24bea",
        "http://purl.obolibrary.org/obo/chebi#59999" ;
    rdfs:subClassOf :ChemicalEntity .

:DiffusionCofficientElectrons a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:DiffusionCofficientHoles a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:ElectronDriftVelocity a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:ElectronIonizationRate a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:ElectronThermalVelocity a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:HoleIonizationRate a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:HoleThermalVelocity a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:InfraredRefractiveIndex a owl:Class ;
    rdfs:subClassOf :OpticalProperty .

:IntrinsicSemiconductor a owl:Class ;
    rdfs:subClassOf :SemiconductorKind .

:Material a owl:Class ;
    rdfs:label "Material" ;
    rdfs:seeAlso "http://emmo.info/emmo#EMMO_4207e895_8b83_4318_996a_72cfb32acd94" ;
    rdfs:subClassOf :Matter .

:MechanicalProperty a owl:Class ;
    rdfs:subClassOf :Property .

:MeltingPoint a owl:Class ;
    rdfs:subClassOf :ThermalProperty .

:MobilityElectrons a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:MobilityHoles a owl:Class ;
    rdfs:subClassOf :ElectricalProperty .

:MolecularEntity a owl:Class ;
    rdfs:seeAlso "http://emmo.info/emmo#EMMO_21205421_5783_4d3e_81e5_10c5d894a88a",
        "http://purl.obolibrary.org/obo/chebi#23367" ;
    rdfs:subClassOf :ChemicalEntity .

:N-TypeSemiconductor a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass [ a owl:Restriction ;
                    owl:onClass :Donor ;
                    owl:onProperty :hasDopingParticipant ;
                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
            owl:onProperty :hasDopingRelation ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :ExtrincSemiconductor .

:OpticalPhotonEnergy a owl:Class ;
    rdfs:subClassOf :OpticalProperty .

:P-TypeSemiconductor a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass [ a owl:Restriction ;
                    owl:onClass :Acceptor ;
                    owl:onProperty :hasDopingParticipant ;
                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
            owl:onProperty :hasDopingRelation ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :ExtrincSemiconductor .

:RadiativeRecombinationCofficient a owl:Class ;
    rdfs:subClassOf :OpticalProperty .

:SpecificHeat a owl:Class ;
    rdfs:subClassOf :ThermalProperty .

:StaticDielectricConstant a owl:Class ;
    rdfs:subClassOf :OpticalProperty .

:TemperatureCondition a owl:Class ;
    rdfs:subClassOf <http://qudt.org/schema/qudt/Quantity>,
        :Condition .

:ThermalConductivity a owl:Class ;
    rdfs:subClassOf :ThermalProperty .

:ThermalDiffusivity a owl:Class ;
    rdfs:subClassOf :ThermalProperty .

:ThermalExpansion a owl:Class ;
    rdfs:subClassOf :ThermalProperty .

:hasAcceptor a owl:ObjectProperty .

:hasCondition a owl:ObjectProperty ;
    rdfs:domain :Experiment ;
    rdfs:range :Condition .

:hasDonor a owl:ObjectProperty .

:hasExperimentalInput a owl:ObjectProperty ;
    rdfs:domain :Experiment .

:hasExperimentalOutput a owl:ObjectProperty ;
    rdfs:domain :Experiment .

:hasSemiconductorType a owl:ObjectProperty ;
    rdfs:domain :SemiconductorMaterial ;
    rdfs:range :SemiconductorKind .

:relatesToMaterial a owl:ObjectProperty ;
    rdfs:domain :SemiconductorStructure ;
    rdfs:range :SemiconductorMaterial ;
    rdfs:seeAlso "https://w3id.org/mdo/core/relatesToMaterial" .

<https://w3id.org/mdo/core/Calculation> a owl:Class .

:ExperimentalEquipment a owl:Class ;
    rdfs:subClassOf prov:Agent .

:SemiconductorStructure a owl:Class .

prov:Agent a owl:Class .

prov:wasAssociatedWith a owl:ObjectProperty .

:ChemicalEntity a owl:Class ;
    rdfs:subClassOf :Matter .

:ExtrincSemiconductor a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :DopingRelation ;
            owl:onProperty :hasDopingRelation ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :SemiconductorKind .

:Matter a owl:Class ;
    rdfs:label "Matter" ;
    rdfs:comment "A matter is a physical substance." ;
    rdfs:seeAlso "http://emmo.info/emmo#EMMO_5b2222df_4da6_442f_8244_96e9e45887d1" .

:SemiconductorMaterial a owl:Class .

<http://qudt.org/schema/qudt/Quantity> a owl:Class .

:Acceptor a owl:Class ;
    rdfs:subClassOf :Dopant .

:Condition a owl:Class .

:Donor a owl:Class ;
    rdfs:subClassOf :Dopant .

:Dopant a owl:Class .

:DopingRelation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasDopingParticipant ;
            owl:someValuesFrom :Dopant ],
        [ a owl:Restriction ;
            owl:onProperty :hasDopingParticipant ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( :Acceptor :Donor ) ] ] .

:Experiment a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom :ExperimentalEquipment ] .

:SemiconductorKind a owl:Class .

:hasDopingRelation a owl:ObjectProperty ;
    rdfs:range :DopingRelation .

:Property a owl:Class ;
    rdfs:subClassOf <http://qudt.org/schema/qudt/Quantity> .

:hasDopingParticipant a owl:ObjectProperty ;
    rdfs:domain :DopingRelation ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( :Acceptor :Donor ) ] .

:OpticalProperty a owl:Class ;
    rdfs:subClassOf :Property .

:ThermalProperty a owl:Class ;
    rdfs:subClassOf :Property .

:ElectricalProperty a owl:Class ;
    rdfs:subClassOf :Property .

